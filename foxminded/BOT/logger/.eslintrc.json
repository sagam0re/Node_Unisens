{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "semi": 0,
    "linebreak-style": 0,
    "lines-between-class-members": 0,
    "comma-dangle": 0,
    "consistent-return": 0,
    "object-curly-newline": 0,
    "no-console": 1,
    "import/extensions": 0,
    "no-underscore-dangle": 0,
    "no-undef": 1,
    "no-unused-vars": 1
  }
}
